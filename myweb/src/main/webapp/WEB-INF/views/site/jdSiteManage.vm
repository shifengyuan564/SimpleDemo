    <!-- 查询结果表格 -->
    <div class="row">
        <div class="col-md-12 col-xs-12 noPaddingLeft noPaddingRight">
            <table id="resultGrid" class="display table-hover" width="100%" cellspacing="0">
                <thead>
                    <th class="switchAll">
                        <input type="checkbox" name="switchAllInput" id="switchAllInput" class="switchAllInput"/>
                    </th>
                    <th style="text-align: left;">区域</th>
                    <th style="text-align: left;">运营中心</th>
                    <th style="text-align: left;">门店名称</th>
                    <th style="text-align: left;">业务编号</th>
                    <th style="text-align: left;">省</th>
                    <th style="text-align: left;">市</th>
                    <th style="text-align: left;">县</th>
                    <th style="text-align: left;">管家</th>
                    <th style="text-align: left;">店长</th>
                    <th style="text-align: left;">商城账号</th>
                    <th style="text-align: left;">门店级别</th>
                    <th style="text-align: left;">门店状态</th>
