                            <option value="20">20</option>
                            <option value="30">30</option>
                        </select>
                        <div class="btnseparator"></div>
                        <a class="glyphicon glyphicon-step-backward firstPage" aria-hidden="true"></a>
                        <a class="glyphicon glyphicon-backward prevPage" aria-hidden="true"></a>
                        <div class="btnseparator"></div>
                        <div class="pageNoInputContainer">
                            <span style="float:left;margin-left:5px;">Page</span>
                            <input name="pageNo" id="pageNo" placeholder="" class="form-control input-sm pageNoInput" value="1">
                            <span style="float:left;margin-left:5px;">of</span>
                            <span style="float:left;margin-left:5px;" class="totalPage">1</span>
                        </div>

                        <div class="btnseparator"></div>
                        <a class="glyphicon glyphicon-forward nextPage" aria-hidden="true"></a>
                        <a class="glyphicon glyphicon-step-forward lastPage" aria-hidden="true"></a>
                        <div class="btnseparator"></div>
                        <a class="glyphicon glyphicon-refresh refreshBtn" aria-hidden="true"></a>
                        <div class="btnseparator"></div>

                        <span class="footerInfo">当前显示从1到15条记录,共0条</span>

                        <input class="currentPage" type="hidden" value="1"/>
                        <input class="nextPageInput" type="hidden" value="1"/>
                        <input class="prevPageInput" type="hidden" value="0"/>
                    </div>
                </td>
                <tfoot>
            </table>
            <input type="hidden" id="deleteHiddenInput"/>
        </div>
    </div>
</div>

